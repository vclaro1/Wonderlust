<div class="transitions-enabled" id="trips">
    <%@users.each do |usr|%>
      <div class="box panel panel-default">
        <div class="col-md-3"> <%= link_to (image_tag usr.image.url(:medium)), usr %></div>
        <div class="panel-body">
          <h3><%= link_to usr.name, usr%></h3>
          <p class="user">User since <%=usr.created_at%></p>
        </div>
        <div class="panel-footer">
          <div class="row">
            <div class="col-md-6">
              <div class="btn-group pull-right">
                  <span class="glyphicon glyphicon-thumbs-up"></span>
              </div>
            </div>
          </div>
    <% end %>
</div>
